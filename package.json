{
  "name": "coffeetest",
  "displayName": "testcafe Test Explorer",
  "description": "Run your testcafe tests in the Sidebar of Visual Studio Code",
  "icon": "img/icon.png",
  "author": "Romer Chavez <romerchp@gmail.com>",
  "publisher": "hbenl",
  "version": "0.0.1",
  "license": "MIT",
  "homepage": "https://github.com/R0m3rCh/CoffeeTest",
  "repository": {
    "type": "git",
    "url": "https://github.com/R0m3rCh/CoffeeTest.git"
  },
  "bugs": {
    "url": "https://github.com/R0m3rCh/CoffeeTest/issues"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "test",
    "testing"
  ],
  "main": "out/main.js",
  "scripts": {
    "clean": "rimraf out *.vsix",
    "build": "tsc",
    "watch": "tsc -w",
    "rebuild": "npm run clean && npm run build",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "dependencies": {
    "testcafe": "^2.5.0",
    "testcafe-browser-tools": "^2.0.23",
    "tslib": "^1.9.3",
    "vscode-test-adapter-api": "^1.7.0",
    "vscode-test-adapter-util": "^0.7.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.77.0",
    "typescript": "^4.9.5",
    "vsce": "^1.95.1"
  },
  "engines": {
    "vscode": "^1.23.0"
  },
  "extensionDependencies": [
    "hbenl.vscode-test-explorer"
  ],
  "activationEvents": [
    "workspaceContains:*.ts"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "TestCafe Test Explorer configuration",
      "properties": {
        "testcafeExplorer.logpanel": {
          "description": "write diagnotic logs to an output panel",
          "type": "boolean",
          "scope": "resource"
        },
        "testcafeExplorer.logfile": {
          "description": "write diagnostic logs to the given file",
          "type": "string",
          "scope": "resource"
        }
      }
    },
    "views": {
      "test": [
        {
          "type": "webview",
          "id": "coffeeTest.settingsView",
          "name": "CoffeeTest"
        }
      ]
    }
  }
}
